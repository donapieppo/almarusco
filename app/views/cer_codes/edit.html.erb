<%= render partial: 'form' %>  

<dl>
  <dt class="my-2">Elenco rifiuti <strong><%= @year %></strong></dt>
  <dd>
    <% @cer_code.disposals(current_organization).select(:id).where("YEAR(created_at) = ?", @year).each do |disposal| %>
      <%= link_to disposal.id, disposal_path(disposal, modal: 1), data: {turbo_frame: "modal"}, class: "d-inline-block btn btn-sm btn-info " %>
    <% end %>
  </dd>
</dl>
