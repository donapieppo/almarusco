<%= dm_form_for [@picking, @picking_document], title: @picking.to_s do |f| %>
  <div class="offset-sm-3 alert alert-info">
    <%= @picking_document.disposal_type.to_s_complete %>
  </div>

  <%= f.input :disposal_type_id, as: :hidden %>  
  <%= f.input :serial_number %>  

  <%= f.input :kgs %>  
  <%= f.input :volume %>  
  <%= f.submit %> - <%= link_to_back @picking %>    
<% end %>
